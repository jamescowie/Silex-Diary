{
    "name": "jamescowie/diary",
    "description": "Simple silex diary application",
    "license": "MIT",
    "type": "project",
    "require": {
        "php": ">=5.3.3",
        "silex/silex": "~1.0",
        "symfony/twig-bridge": "~2.3",
        "filp/whoops": "1.*",
        "symfony/yaml":"v2.2.0",
        "symfony/config":"~2",
        "symfony/form": "~2.3",
        "symfony/validator": "~2.3",
        "symfony/translation": "~2.3",
        "taluu/doctrine-orm-provider": "1.0.*@dev"
    },
    "require-dev": {
        "behat/behat": "~3",
        "phpspec/phpspec": "~2",
        "squizlabs/php_codesniffer": "1.*",
        "phpmd/phpmd": "1.*",
        "sebastian/phpcpd": "2.*",
        "pdepend/pdepend": "1.*",
        "bossa/phpspec2-expect": "dev-master",
        "behat/mink": "*",
        "behat/mink-extension": "*",
        "behat/mink-goutte-driver": "*",
        "behat/mink-selenium2-driver": "*",
        "symfony/debug": "~2.3"
    },
    "autoload": {
        "psr-0": { "Diary": "src/" }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        },
        "heroku": {
            "document-root": "web",
            "index-document": "web/index.php"
        }
    },
    "config": {
        "bin-dir": "bin"
    }
}
